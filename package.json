{
  "name": "xzl-webs",
  "version": "1.0.0",
  "description": "",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "author": "",
  "lint-staged": {
    "*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "preinstall": "pnpm i -g commitizen --registry=https://registry.npm.taobao.org",
    "postinstall": "husky install && commitizen init cz-conventional-changelog-zh",
    "lint": "eslint --ext ts,tsx packages/**/src/ --fix",
    "cz": "git add . && git commit",
    "clean:doctor": "pnpm clean --filter 'xzl-web-doctor'",
    "clean:nurse": "pnpm clean --filter 'xzl-web-nurse'",
    "clean:org": "pnpm clean --filter 'xzl-web-org'",
    "clean:out": "pnpm clean --filter 'out-hospital-patient'",
    "clean:cro": "pnpm clean --filter 'clinical-cro'",
    "dev:doctor": "pnpm dev --filter 'xzl-web-doctor'",
    "dev:nurse": "pnpm dev --filter 'xzl-web-nurse'",
    "dev:org": "pnpm dev --filter 'xzl-web-org'",
    "dev:out": "pnpm dev --filter 'out-hospital-patient'",
    "dev:cro": "pnpm dev --filter 'clinical-cro'",
    "dev-shared:doctor": "pnpm dev-shared --filter 'xzl-web-doctor'",
    "dev-shared:nurse": "pnpm dev-shared --filter 'xzl-web-nurse'",
    "dev-shared:org": "pnpm dev-shared --filter 'xzl-web-org'",
    "dev-shared:out": "pnpm dev-shared --filter 'out-hospital-patient'",
    "dev-shared:cro": "pnpm dev-shared --filter 'clinical-cro'",
    "debug:doctor": "pnpm debug --filter 'xzl-web-doctor'",
    "debug:nurse": "pnpm debug --filter 'xzl-web-nurse'",
    "debug:org": "pnpm debug --filter 'xzl-web-org'",
    "debug:out": "pnpm debug --filter 'out-hospital-patient'",
    "debug:cro": "pnpm debug --filter 'clinical-cro'",
    "debug-prod:doctor": "pnpm debug-prod --filter 'xzl-web-doctor'",
    "debug-prod:nurse": "pnpm debug-prod --filter 'xzl-web-nurse'",
    "debug-prod:org": "pnpm debug-prod --filter 'xzl-web-org'",
    "debug-prod:out": "pnpm debug-prod --filter 'out-hospital-patient'",
    "debug-prod:cro": "pnpm debug-prod --filter 'clinical-cro'",
    "prerelease:doctor": "pnpm prerelease --filter 'xzl-web-doctor'",
    "prerelease:nurse": "pnpm prerelease --filter 'xzl-web-nurse'",
    "prerelease:org": "pnpm prerelease --filter 'xzl-web-org'",
    "prerelease:out": "pnpm prerelease --filter 'out-hospital-patient'",
    "prerelease:cro": "pnpm prerelease --filter 'clinical-cro'",
    "dist:doctor": "pnpm dist --filter 'xzl-web-doctor'",
    "dist:nurse": "pnpm dist --filter 'xzl-web-nurse'",
    "dist:org": "pnpm dist --filter 'xzl-web-org'",
    "dist:out": "pnpm dist --filter 'out-hospital-patient'",
    "dist:cro": "pnpm dist --filter 'clinical-cro'",
    "dev-dist:doctor": "pnpm dev-dist --filter 'xzl-web-doctor'",
    "dev-dist:nurse": "pnpm dev-dist --filter 'xzl-web-nurse'",
    "dev-dist:org": "pnpm dev-dist --filter 'xzl-web-org'",
    "dev-dist:out": "pnpm dev-dist --filter 'out-hospital-patient'",
    "dev-dist:cro": "pnpm dev-dist --filter 'clinical-cro'",
    "build-ts-watch:shared": "cd packages/shared && pnpm build-ts-watch"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.8",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-zh": "0.0.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "7.0.2",
    "lint-staged": "^11.1.2",
    "postcss": "7",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "typescript": "^4.4.2"
