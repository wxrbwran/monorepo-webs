{
  "name": "out-hospital-patient",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "clean": "rimraf ./dist ./src/.umi ./src/.umi-producation",
    "postinstall": "pnpm clean",
    "todev": "./config/todev.sh",
    "toprod": "./config/toprod.sh",
    "dev": "pnpm todev && NODE_ENV=development APP_ENV=dev umi dev",
    "debug": "pnpm todev && NODE_ENV=development APP_ENV=test umi dev",
    "debug-prod": "pnpm todev && NODE_ENV=development APP_ENV=prod umi dev",
    "dev-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "debug-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "dev-dist": "pnpm toprod && NODE_ENV=production APP_ENV=dev umi build",
    "prerelease": "pnpm toprod && NODE_ENV=production APP_ENV=test umi build",
    "dist": "pnpm toprod && NODE_ENV=production APP_ENV=prod umi build",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "site": "npm run fetch:blocks && npm run build",
    "start": "umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env umi dev",
    "start:test": "cross-env MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "docker-hub:build": "docker build  -f Dockerfile.hub -t  ant-design-pro ./",
    "docker-prod:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker-prod:dev": "docker-compose -f ./docker/docker-compose.yml up",
    "docker:build": "docker-compose -f ./docker/docker-compose.dev.yml build",
    "docker:dev": "docker-compose -f ./docker/docker-compose.dev.yml up",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push antdesign/ant-design-pro",
    "docker:tag": "docker tag ant-design-pro antdesign/ant-design-pro",
    "gh-pages": "gh-pages -d dist",
    "lint": "eslint --ext ts,tsx src/ --fix",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "yarn lint"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@alitajs/keep-alive": "^2.5.14",
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-descriptions": "^1.0.19",
    "@ant-design/pro-form": "^1.0.2",
    "@ant-design/pro-layout": "^6.4.19",
    "@ant-design/pro-table": "^2.9.5",
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@types/redux-logger": "^3.0.8",
    "@umijs/fabric": "^2.2.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.0-beta.2",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/route-utils": "^1.0.33",
    "@umijs/yorkie": "^2.0.3",
    "antd": "^4.16.13",
    "autoprefixer": "^9.8.6",
    "classnames": "^2.2.6",
    "cropperjs": "^1.5.9",
    "cross-env": "^7.0.3",
    "dayjs": "^1.9.3",
    "dva": "^2.6.0-beta.6",
    "dva-model-persist": "^1.0.0",
    "husky": "^4.3.8",
    "immutability-helper": "^3.1.1",
    "js-base64": "^3.6.0",
    "lodash": "^4.17.11",
    "mockjs": "^1.1.0",
    "moment": "^2.25.3",
    "omit.js": "^2.0.2",
    "postcss": "^7.0.35",
    "qs": "^6.9.0",
    "react": "^16.8.6",
    "react-cropper": "^2.1.4",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.8.6",
    "react-draggable": "^4.4.3",
    "react-helmet-async": "^1.0.4",
    "react-quill": "^1.3.5",
    "react-viewer": "^3.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "rimraf": "^3.0.2",
    "store": "^2.0.12",
    "umi": "^3.5.17",
    "umi-request": "^1.0.8",
    "use-merge-value": "^1.0.1",
    "@tailwindcss/postcss7-compat": "2.2.9",
    "xzl-web-shared": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@types/classnames": "^2.2.7",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^6.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "carlo": "^0.9.46",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "pro-download": "1.0.1",
    "stylelint": "^13.0.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "create-umi": {
    "ignoreScript": [
      "docker*",
      "functions*",
      "site",
      "generateMock"
    ],
    "ignoreDependencies": [
      "netlify*",
      "serverless"
    ],
    "ignore": [
      ".dockerignore",
      ".git",
      ".github",
      ".gitpod.yml",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "Dockerfile.*",
      "lambda",
      "LICENSE",
      "netlify.toml",
      "README.*.md",
      "azure-pipelines.yml",
      "docker",
      "CNAME",
      "create-umi"
    ]
  }
}
