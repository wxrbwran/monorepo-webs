{
  "name": "xzl-web-shared",
  "version": "1.0.0",
  "description": "",
  "main": "dist",
  "keywords": [],
  "author": "",
  "scripts": {
    "clean": "",
    "lint": "eslint --ext js,ts,tsx src",
    "build-project": "pnpm clean && pnpm build-ts && pnpm build-css",
    "build-watch": "pnpm clean && pnpm build-ts-watch && pnpm build-css-watch",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "sass ./src/components:./dist/components",
    "build-watch-all": "concurrently 'pnpm build-ts-watch' 'pnpm build-css-watch' ",
    "build-ts-watch": "tsc -p tsconfig.build.json --watch",
    "build-css-watch": "sass ./src/components:./dist/components --watch",
    "image-min-watch": "pnpm image-min && onchange './src/assets/**/*.(png|jpg|jpeg|svg)' -- pnpm image-min",
    "image-min": "node ./image.mjs",
    "image-copy-watch": "pnpm image-copy && onchange './src/assets/' -- pnpm image-copy",
    "image-copy": "copyfiles ./src/assets/ ./dist/assets/",
    "prepublishOnly": "pnpm lint && pnpm build-project",
    "fuzhi-pag": "pnpm fuzhi package-dist.json dist/package.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.11.2",
    "antd": "4.17.0",
    "axios": "^0.21.3",
    "classnames": "^2.2.6",
    "css-loader": "^5.2.6",
    "dayjs": "^1.10.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react-draggable": "^4.4.3",
    "react-transition-group": "^4.4.1",
    "store": "^2.0.12",
    "style-loader": "^2.0.0",
    "umi-request": "^1.0.8"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-transition-group": "^4.4.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.3",
    "file-type": "^16.5.3",
    "fork-ts-checker-webpack-plugin": "^5.0.13",
    "fuzhi": "^1.0.0",
    "globby": "^12.0.2",
    "graceful-fs": "^4.2.8",
    "husky": "4.3.8",
    "imagemin": "^8.0.1",
    "junk": "^4.0.0",
    "less-loader": "^9.0.0",
    "onchange": "^7.1.0",
    "p-pipe": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-ext": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass": "1.38.2",
    "slash": "^4.0.0",
    "ts-loader": "^8.0.2",
    "typescript": "4.2.4",
    "webpack-combine-loaders": "^2.0.4"
  }
}