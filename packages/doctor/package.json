{
  "name": "xzl-web-doctor",
  "description": "心之力医生端",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist ./src/.umi ./src/.umi-producation",
    "todev": "./config/todev.sh",
    "toprod": "./config/toprod.sh",
    "dev": "pnpm todev && NODE_ENV=development APP_ENV=dev umi dev",
    "debug": "pnpm todev && NODE_ENV=development APP_ENV=test umi dev",
    "debug-prod": "pnpm todev && NODE_ENV=development APP_ENV=prod umi dev",
    "dev-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "debug-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "dev-dist": "pnpm toprod && NODE_ENV=production APP_ENV=dev umi build",
    "prerelease": "pnpm toprod && NODE_ENV=production APP_ENV=test umi build",
    "dist": "pnpm toprod && NODE_ENV=production APP_ENV=prod umi build",
    "build": "umi build",
    "test": "umi test --watch",
    "lint": "eslint --ext ts,tsx src/ --fix",
    "precommit": "lint-staged",
    "postinstall": "umi g tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "pnpm lint"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.4",
    "@ant-design/pro-layout": "^6.25.1",
    "@tailwindcss/postcss7-compat": "2.2.9",
    "@types/dva": "^1.1.0",
    "@types/redux-mock-store": "^1.0.3",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "^1.8.22",
    "antd": "^4.16.13",
    "autoprefixer": "^9",
    "axios": "^0.21.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.3.1",
    "core-js": "^3.16.4",
    "cropperjs": "^1.5.12",
    "dayjs": "^1.10.6",
    "dva": "^2.6.0-beta.6",
    "dva-model-persist": "^1.0.0",
    "express": "^4.17.1",
    "html2canvas": "^1.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "postcss": "^7",
    "qrcode": "^1.4.4",
    "qrcode.react": "^1.0.1",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-slick": "^0.28.1",
    "react-viewer": "^3.2.2",
    "redux-logger": "^3.0.6",
    "regenerator-runtime": "^0.13.9",
    "store": "^2.0.12",
    "to": "^0.2.9",
    "umi": "3.5.17",
    "umi-plugin-tailwindcss": "^3.1.5",
    "umi-request": "^1.3.9",
    "umi-types": "^0.5.14",
    "update": "^0.7.4",
    "xzl-web-shared": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.8",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^27.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-umi": "^1.6.0",
    "eslint-plugin-flowtype": "^5.9.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "7.0",
    "jest": "^27.1.0",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.1.0",
    "typescript": "^4.4.2",
    "umi-webpack-bundle-analyzer": "^4.4.2"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "installConfig": {
    "pnp": true
  }
}
