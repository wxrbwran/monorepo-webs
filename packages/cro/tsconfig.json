{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./src/.umi/*"]
    },
    "typeRoots": [
      "node",
      "node_modules/@types",
      "./typings"
    ],
  },
  "include": ["src/**/*", "config/**/*"],
  "exclude": ["node_modules", "build", "dist", "scripts", "src/.umi/*", "webpack", "jest"]
}
