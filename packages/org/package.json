{
  "name": "xzl-web-org",
  "description": "心之力管理员端",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist ./src/.umi ./src/.umi-producation",
    "postinstall": "pnpm clean",
    "todev": "./config/todev.sh",
    "toprod": "./config/toprod.sh",
    "dev": "pnpm todev && NODE_ENV=development APP_ENV=dev umi dev",
    "debug": "pnpm todev && NODE_ENV=development APP_ENV=test umi dev",
    "debug-prod": "pnpm todev && NODE_ENV=development APP_ENV=prod umi dev",
    "dev-dist": "pnpm toprod && NODE_ENV=production APP_ENV=dev umi build",
    "prerelease": "pnpm toprod && NODE_ENV=production APP_ENV=test umi build",
    "dist": "pnpm toprod && NODE_ENV=production APP_ENV=prod umi build",
    "dev-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "debug-shared": "pnpm clean && pnpm toprod && NODE_ENV=development APP_ENV=dev umi dev",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test:coverage": "umi-test --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
